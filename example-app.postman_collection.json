	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"",
					"let baseUrl = pm.collectionVariables.get(\"base_url\");",
					"",
					"pm.sendRequest({",
					"    url: baseUrl + '/sanctum/csrf-cookie',",
					"    method: 'GET',",
					"}, function(error, response, { cookies }) {",
					"        console.log(response)",
					"    if(!error) {",
					"        pm.collectionVariables.set('xsrf-cookie', cookies.get('XSRF-TOKEN'));",
					"    } else {",
					"        console.log(error)",
					"    }",
					"});"
				]
			}
		},
